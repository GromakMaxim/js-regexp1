{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style":"off",
        "no-plusplus":"off",
        "no-restricted-syntax":"off",
        "guard-for-in":"off",
        "import/extensions":"off",
        "no-param-reassign":"off",
        "class-methods-use-this":"off"
    }
}